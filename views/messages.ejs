<ul>
  <div className="MessageWrapper">
    <% for (let i = 0; i < messages.length; i++) { %>
    <div>
      <h1 className="user">
        <a href="/message/<%= i %>"> <%= messages[i].user %> </a>
      </h1>
      <p><%= messages[i].added %></p>
    </div>
    <span className="message"> <%= messages[i].text %> </span>
    <% } %>
  </div>
</ul>
