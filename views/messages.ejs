<ul>
  <div class="MessageWrapper">
    <% for (let i = 0; i < messages.length; i++) { %>
    <div>
      <h1 class="user">
        <a href="/message/<%= i %>"><%= messages[i].username %></a>
      </h1>
      <p><%= messages[i].date %></p>
    </div>
    <span class="message"><%= messages[i].message %></span>
    <% } %>
  </div>
</ul>
